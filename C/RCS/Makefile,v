all: $(EXECS)

#Deleta todos arquivos instalados
clean: 
	rm -f *.o
	rm -f *.cgi
	rm geCLI

#Instala a braganÃ§a toda
install: 
	mkdir ../../../../../public/html/EEL270
	mkdir ../../../../../public/html/EEL270/GE
	mkdir ../../../../../public/html/EEL270/GE/CGIs
	mkdir ../../../../../public/html/EEL270/GE/Files
	mkdir ../../../../../public/html/EEL270/GE/Files/Cookies
	co -l geCrypt.c geError.c geFunctions.c geConst.h geCrypt.h geError.h geFunctions.h geTypes.h 
	co -l mlcgi.h sendmail.h
	cp -f ../HTML/*.html ../../../../../public/html/EEL270/GE/
	cp -f ../HTML/Files/* ../../../../../public/html/EEL270/GE/Files/
	make executeinstall
	../../../../../public/html/EEL270/GE/geCLI -z
	
executeinstall: $(EXECSCO)
	make
	make clean
	
deinstall:
	rm -rf ../../../../../public/html/EEL270/
	
#Compilacao de todos arquivos

